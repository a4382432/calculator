   #include <stdio.h>
   #include <string.h>
   int main()
   {
           char a[50],b[]={"Hello"};
  
           scanf("%[^\n]",a);
           getchar();
           if((strcmp(a,"var")==0)||(strcmp(a,"VAR"))==0)
          {if(b[0]!='\0')
                  printf("b = %s",b);}
 
      return 0;
  }
